{
  "expo": {
    "name": "AceQuint",
    "slug": "acequint-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/icon.png",
    "scheme": "acequint",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2D1B16"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.acequint.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/icon.png",
        "backgroundColor": "#2D1B16"
      },
      "package": "com.acequint.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.RECORD_AUDIO"
      ],
      "usesCleartextTraffic": true,
      "networkSecurityConfig": "network_security_config"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./app/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-image-picker",
      "expo-camera"
    ],
    "experiments": {
      "typedRoutes": false
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b3772d3e-f1fd-4184-9a4d-f212b07469ff"
      }
    },
    "owner": "prabheen09876"
  }
}
