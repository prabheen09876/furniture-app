{
  "expo": {
    "name": "Kesarwala",
    "slug": "kesarwala-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logokesarwala.png",
    "scheme": "kesarwala",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF8C00"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kesarwala.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logokesarwala.png",
        "backgroundColor": "#F5F0E8"
      },
      "package": "com.kesarwala.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/logokesarwala.png"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker",
      "expo-camera",
      "expo-font",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": false
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7e2944d5-2988-4577-aa7b-9ac676be994b"
      }
    },
    "owner": "prabheen09876"
  }
}
